meta {
  name: bk-legacy-invalid-params-querysting
  type: http
  seq: 3
}

get {
  url: {{scheme}}://{{host}}/api/smoke/prod/bk-legacy-invalid-params?a=1&amp;b=2
  body: none
  auth: none
}

params:query {
  a: 1
  amp;b: 2
}

headers {
  X-Bkapi-Authorization: {"access_token": "{{access_token}}"}
}

assert {
  res.status: eq 200
  res.body.args["a"]: eq "1"
  res.body.args["b"]: eq "2"
}
